{
    “manifest_version”: 2,
    “name”: “Twindle Browser Extension”,
    “description”: "Convert Twitter threads to PDF",
    "permissions":[
         "clipboardRead",
         "storage"
    ],
    "icons": {
        "128": "./icon.svg"
    },
    "browser_action": {
        "default_icon": "icon.svg",
        "default_title": "Twindle Browser Extension",
        "default_popup": "index.html"
    },
}
