    “manifest_version”: 2,
    “name”: “Twindle Browser Extension”,
    “description”: "Convert Twitter threads to PDF",
    "permissions":[
         "clipboardRead",
         "storage"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "64": "icons/icon-64.png",
        "86": "icons/icon-86.png",
        "128":"icons/icon-128.png"
    }
}
